  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.23",
    "@types/ncp": "^2.0.1",
    "arg": "^4.1.1",
    "await-exec": "^0.1.2",
    "ejs": "^2.7.1",
    "execa": "^2.0.4",
    "express": "^4.17.1",
    "inquirer": "^7.0.0",
